{
  "name": "orion-portal",
  "version": "0.1.0",
  "private": true,
  "description": "ORION Self-Service Portal - P6 + SAP Data Visualization",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run --dir src",
    "test:integration": "vitest run --dir tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:a11y": "vitest run --dir tests/a11y",
    "test:perf": "vitest run --dir tests/perf",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:a11y",
    "docs:sync": "python ../../../.claude/hooks/sync_component_docs.py --sync",
    "docs:check": "python ../../../.claude/hooks/sync_component_docs.py --check",
    "gate:check": "npm run test:coverage && npm run test:e2e && npm run test:a11y",
    "brain:log": "echo 'Logging to Corporate Brain...'"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.12.0",
